{
  "name": "art-gallery",
  "version": "1.0.0",
  "description": "Art Gallery built with Parcel",
  "homepage": "https://gautamjain0909.github.io/Art-Gallery",
  "scripts": {
    "start": "parcel index.html",
    "prebuild-dev": "shx rm -rf dist/*",
    "clean": "rm -rf dist/*",
    "build-dev": "parcel build index.html --no-optimize --public-url ./",
    "prebuild": "shx rm -rf dist/*",
    "build:parcel": "parcel build index.html --no-content-hash --no-source-maps --public-url ./",
    "build": "parcel build index.html --public-url ./",
    "push-gh-pages": "push-dir --dir=dist --branch=gh-pages --cleanup --verbose",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "author": "Gautam Jain",
  "license": "ISC",
  "dependencies": {
    "gsap": "^3.6.1",
    "imagesloaded": "^4.1.4",
    "locomotive-scroll": "^3.6.1",
    "parcel": "latest",
    "splitting": "^1.0.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gautamjain0909/Art-Gallery.git"
  },
  "bugs": {
    "url": "https://github.com/gautamjain0909/Art-Gallery/issues"
  },
  "devDependencies": {
    "gh-pages": "^6.1.1",
    "parcel-bundler": "^1.12.5",
    "shx": "^0.3.2"
  }
}
